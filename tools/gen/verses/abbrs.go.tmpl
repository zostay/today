package {{.Package}}

import "github.com/zostay/today/pkg/bible"

var Abbreviations = &bible.BookAbbreviations{
    Abbreviations: []bible.BookAbbreviation{
{{- range .Abbreviations}}
        {
            Name: "{{.Name}}",
            Preferred: "{{.Standard}}",
{{- if .Singular}}
            Singular: "{{.Singular}}",
{{- end }}
{{- if .Ordinal}}
            Ordinal: {{.Ordinal}},
{{- end}}
            Accepts: []string{
{{- range .Accept}}
                "{{.}}",
{{- end}}
            },
        },
{{- end}}
    },
}
